{
    "id": "nodebb-plugin-xbone",
    "name": "XBone plugin",
    "description": "Replaces common misspellings of Xbone (e.g. Xbox One)",
    "url":"https://github.com/Kern--/nodebb-plugin-xbone",
    "library":"./library.js",
    "hooks": [
        {"hook":"filter:parse.raw", "method":"xbonifyRaw"},
        {"hook":"filter:parse.post", "method":"xbonifyPost"},
        {"hook":"filter:parse.signature", "method":"xbonifySignature"}
    ]
}